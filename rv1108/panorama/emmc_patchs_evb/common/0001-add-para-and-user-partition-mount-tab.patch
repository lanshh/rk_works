 root/etc/fstab | 2 ++
 1 file changed, 2 insertions(+)

diff --git a/root/etc/fstab b/root/etc/fstab
index 4a53a5b..d42438c 100644
--- a/root/etc/fstab
+++ b/root/etc/fstab
@@ -4,3 +4,5 @@ none    /var    ramfs   defaults 0 0
 none    /dev    devtmpfs   size=4k 0 0
 sysfs   /sys    sysfs   defaults 0 0
 debug   /sys/kernel/debug debugfs defaults 0 0
+/dev/mmcblk0p5 /param   ext4   defaults    0   0
+/dev/mmcblk0p6 /user   ext4   defaults    0   0
